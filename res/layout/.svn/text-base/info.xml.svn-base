<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:orientation="vertical" android:layout_height="wrap_content">
    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/babyNameTextView" android:editable="false"></TextView>
    <TextView android:text="@string/outputDateLabel" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="top" android:layout_weight=".1" android:id="@+id/dueDateTextView"></TextView>
    <EditText android:layout_weight="1" android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/outputDateTextBox" android:editable="false"></EditText>
    <TextView android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/farAlongTextView" android:text=" "></TextView>
    <TextView android:id="@+id/toGoTextView" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=" "></TextView>
    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:fadeScrollbars="true" android:text="Baby looks like:" android:id="@+id/progressImage"></TextView>
    <ImageView android:id="@+id/fiv" android:layout_width="200dp" android:layout_height="200dp"></ImageView>
    <TextView android:id="@+id/fDesc" android:layout_height="wrap_content" android:layout_width="wrap_content"></TextView>
</LinearLayout>
